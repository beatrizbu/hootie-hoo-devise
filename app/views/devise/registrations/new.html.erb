<!-- <div class="container-full"> -->
  <div class="jumbotron">
    <div class="row">
      <h1 class="title">Hootie-Hoo</h1>
        <h2 class="motto">Breaking the ice, one hoot at a time!</h2>
        <div class="col-md-4 col-md-offset-4 col-xs-4 col-xs-offset-4">
          <div class="hootie-hoot">
              <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
              <%= devise_error_messages! %>
              <div class="form-group">
                <%= f.label :name %>
                <%= f.text_field :name, autofocus: true, class: "form-control" %></div>

              <div class="form-group">
                <%= f.label :email %>
                <%= f.email_field :email, class: "form-control" %></div>
              <% if f.object.password_required? %>
                <div class="form-group">
                <%= f.label :password %>
                <%= f.password_field :password, autocomplete: "off", class: "form-control" %></div>

                <div class="form-group">
                <%= f.label :password_confirmation %>
                <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control" %></div>
              <% end %>

              <ul>
                <div><%= f.submit "Sign Up", class: "btn btn-primary" %>
                <%= link_to "Facebook Sign In", user_omniauth_authorize_path(:facebook), class: "btn btn-primary" %>
                </div>
                <br>
              </ul>
              <% end %>


    <!-- <%= render "devise/shared/links" %> -->
            <!-- </div> -->
        </div>
      </div>
  </div>
</div>
